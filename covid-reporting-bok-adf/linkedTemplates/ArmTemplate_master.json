{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-reporting-bok-adf"
		},
		"ls_ablob_covidreportingblobbk_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidreportingblobbk'"
		},
		"ls_adls_covidreportingdlbk_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportingdlbk'"
		},
		"ls_adls_covidreportingdlbk_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingdlbk.dfs.core.windows.net/"
		},
		"ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"tg_copy_population_file_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/b6836ef6-15b8-4efc-af78-ebde2bfe9173/resourceGroups/covid-reporting-rg/providers/Microsoft.Storage/storageAccounts/covidreportingblobbk"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-reporting-bok-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidreportingblobbk_connectionString": {
						"value": "[parameters('ls_ablob_covidreportingblobbk_connectionString')]"
					},
					"ls_adls_covidreportingdlbk_accountKey": {
						"value": "[parameters('ls_adls_covidreportingdlbk_accountKey')]"
					},
					"ls_adls_covidreportingdlbk_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportingdlbk_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url')]"
					},
					"tg_copy_population_file_properties_typeProperties_scope": {
						"value": "[parameters('tg_copy_population_file_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}